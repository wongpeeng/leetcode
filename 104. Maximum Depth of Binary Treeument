public class Solution {
    public int maxDepth(TreeNode root) {
       if(root==null)
           return 0;
       return 1+Math.max(mDepth(root.left),mDepth(root.right));
    }//
    public static int mDepth(TreeNode root){
        if(root==null)
            return 0;
        return 1+Math.max(mDepth(root.left),mDepth(root.right)); 
    }//
}
